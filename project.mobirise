range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-12\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-section-btn\">Venha saber mais, RESERVE AGORA!</h2>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\"><a class=\"btn btn-primary\" href=\"https://api.whatsapp.com/send?phone=551939383135&text=Ol%C3%A1,%20estou%20vindo%20do%20bot%C3%A3o%20reservas%20do%20site%20da%20Wine%20House,%20gostaria%20de%20fazer%20uma%20reserva%20para%20a%20Degusta%C3%A7%C3%A3o%20de%20Vinhos,%20Cabernet%20Sauvignon%20Pelo%20Mundo.%20Poderia%20me%20ajudar%3F\" data-app-placeholder=\"Type Text\" target=\"_blank\">\n                            <span class=\"mobi-mbri mobi-mbri-arrow-next mbr-iconfont mbr-iconfont-btn\"></span>\n                            FAÇA SUA RESERVA</a></div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
